{
  "name": "wcm-editor",
  "version": "1.0.0",
  "description": "A Editor of WCM code generator",
  "main": "./electron/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --watch",
    "start": "./node_modules/.bin/webpack-dev-server --open",
    "elec": "electron .",
    "elec-local": "./node_modules/electron/cli.js .",
    "packager": "electron-packager . 'Wcm-Editor' --platform=darwin --arch=x64 --icon=app.icns --overwrite=true --out=./dist --app-version=1.0.0 --ignore=\"(src|docs|.gitignore|LICENSE|README.md|webpack.config*)\"",
    "packageDarwin": "electron-packager . 'Wcm-Editor' --platform=darwin --arch=x64 --icon=app.icns --out=./dist --asar --app-version=1.0.0 --ignore=\"(dist|src|docs|.gitignore|LICENSE|README.md|webpack.config*|node_modules)\"",
    "packageDarwin2": "electron-packager . 'Wcm-Editor' --platform=darwin --arch=x64 --icon=app.icns --out=./dist --asar --app-version=1.0.0",
    "packageWin": "electron-packager . 'Wcm-Editor' --platform=win32 --arch=x64 --icon=app.ico --out=./dist --asar --app-version=1.0.0 --ignore=\"(dist|src|docs|.gitignore|LICENSE|README.md|webpack.config.js|node_modules)\"",
    "packageWin2": "electron-packager . 'Wcm-Editor' --platform=win32 --arch=x64 --icon=app.ico --out=./dist --asar --app-version=1.0.0"
  },
  "homepage": ".",
  "author": "Joe.Huanng",
  "license": "ISC",
  "dependencies": {
    "electron-is": "^3.0.0",
    "electron-store": "^3.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-router-dom": "^4.0.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-typescript": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "css-loader": "^2.1.1",
    "electron-packager": "^13.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.3",
    "url-loader": "^2.0.1",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.1.4"
  }
}
